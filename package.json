  "type": "module",
  "exports": "./dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p ./tsconfig.build.json",
    "example:run": "vite example",
    "lint": "tsc --noEmit && prettier --check src example/src && eslint src example/src",
    "lint:fix": "prettier --write src example/src && eslint --fix src example/src",
    "release": "yarn build && np",
    "test": "yarn lint && c8 --reporter=html --reporter=text jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
